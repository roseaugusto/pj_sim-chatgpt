{
  "name": "programming-language-translator",
  "displayName": "Programming Language Translator",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sim-chatgpt-sidebar",
          "title": "Sim chatgpt sidebar",
          "icon": "media/icons/icon.svg"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "submenu": "myextensionsubmenu",
          "group": "navigation"
        }
      ],
      "myextensionsubmenu": [
        {
          "command": "programming-language-translator.runPython",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runAngular",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runC#net",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runTypescript",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runPhp",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runRuby",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runJs",
          "group": "navigation"
        }
      ]
    },
    "submenus": [
      {
        "label": "Testing Framework",
        "id": "myextensionsubmenu"
      }
    ],
    "views": {
      "sim-chatgpt-sidebar": [
        {
          "type": "webview",
          "id": "sim-chatgpt-sidebar",
          "name": "Sim chatgpt",
          "icon": "media/icons/icon.svg",
          "contextualTitle": "sidebar test"
        }
      ]
    },
    "commands": [
      {
        "command": "programming-language-translator.runPhp",
        "title": "PHP"
      },
      {
        "command": "programming-language-translator.runRuby",
        "title": "Ruby"
      },
      {
        "command": "programming-language-translator.runJs",
        "title": "Javascript"
      },
      {
        "command": "programming-language-translator.runPython",
        "title": "Python"
      },
      {
        "command": "programming-language-translator.runAngular",
        "title": "Angular"
      },
      {
        "command": "programming-language-translator.runC#net",
        "title": "C#.net"
      },
      {
        "command": "programming-language-translator.runTypescript",
        "title": "Typescript"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
