{
  "name": "programming-language-translator",
  "displayName": "Programming Language Translator",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sim-chatgpt-sidebar",
          "title": "SIM ChatGPT",
          "icon": "media/icons/icon.svg"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "submenu": "myextensionsubmenu",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "programming-language-translator.runApiKey",
          "group": "navigation"
        }
      ],
      "myextensionsubmenu": [
        {
          "command": "programming-language-translator.runJest",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runJasmine",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runMocha",
          "group": "navigation"
        },
        {
          "command": "programming-language-translator.runAVA",
          "group": "navigation"
        }
      ]
    },
    "submenus": [
      {
        "label": "Create Unit Tests",
        "id": "myextensionsubmenu"
      }
    ],
    "views": {
      "sim-chatgpt-sidebar": [
        {
          "type": "webview",
          "id": "sim-chatgpt-sidebar",
          "name": "Unit Test",
          "icon": "media/icons/icon.svg",
          "contextualTitle": "SIM ChatGPT"
        }
      ]
    },
    "commands": [
      {
        "command": "programming-language-translator.runApiKey",
        "title": "ChatGPT API Key",
        "icon": "media/icons/key.svg"
      },
      {
        "command": "programming-language-translator.runJest",
        "title": "Jest"
      },
      {
        "command": "programming-language-translator.runMocha",
        "title": "Mocha"
      },
      {
        "command": "programming-language-translator.runJasmine",
        "title": "Jasmine"
      },
      {
        "command": "programming-language-translator.runAVA",
        "title": "AVA"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@vscode/test-electron": "^2.3.3",
    "axios": "^1.4.0",
    "openai": "^3.3.0"
  }
}
